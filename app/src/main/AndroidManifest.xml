<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Woofer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mainicon"
        android:label="@string/app_title"
        android:roundIcon="@mipmap/mainicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Woofer">
        <activity android:name=".mutualFriends"/>
        <activity android:name=".pendingRequests" />
        <activity android:name=".createPost" />
        <activity android:name=".viewFriends" />
        <activity android:name=".Add_Friends" />
        <activity android:name=".RegisterView" />
        <activity android:name=".StatusView" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>